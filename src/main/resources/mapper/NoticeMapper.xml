<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.student_serve.mapper.NoticeMapper">

    <resultMap id="BaseResultMap" type="com.student_serve.model.entity.Notice">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="noticeTitle" column="noticeTitle" jdbcType="VARCHAR"/>
            <result property="noticeContent" column="noticeContent" jdbcType="VARCHAR"/>
            <result property="noticeTime" column="noticeTime" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="updateTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,noticeTitle,noticeContent,
        noticeTime,updateTime
    </sql>
</mapper>
